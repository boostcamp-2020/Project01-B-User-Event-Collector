{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
          "components/*"
      ],
      "@utils/*":["utils/*"],
      "@constants/*":["constants/*"],
      "@hooks/*":["hooks/*"],
      "@stores/*":["stores/*"],
      "@interfaces/*":["interfaces/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx", "utils/apis.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
